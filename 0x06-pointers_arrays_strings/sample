#include "main.h"
#include<string.h>
/**
* *_strcat - function
* @dest: will be used
* @src: will be used
* Return: noting
*/
char *_strcat(char *dest, char *src)
{
int n, i, n2, j = 0;
n = strlen(dest);
n2 = strlen(src);
dest[n] = ' ';
for (i = n; i <= n2; i++)
{
dest[i] = src[j];
j++;
}
return (dest);
}

